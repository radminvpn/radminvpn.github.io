$(document).ready(function(){$(document).on({mouseenter:function(){$(this).stop().animate({backgroundColor:"#159ef9"},200)},mouseleave:function(){$(this).stop().animate({backgroundColor:"#3498db"},200)}},".buttonDownload, .buttonSubmit")}),$(document).ready(function(){$(".modal").click(function(n){n.preventDefault(),$(this).modal()})}),$(document).ready(function(){$(".lng").click(function(n){n.preventDefault(),$("#lang").modal()})});